{
  "clickable_minimum_required": "5.7",
  "template": "cmake",
  "kill": "teleports",
  "dependencies_target": [
    "gperf",
    "libssl-dev"
  ],
  "dependencies_build": [
    "ccache"
  ],
  "dir": "build/teleports",
  "make_jobs": "4",
  "libraries": [
    {
      "template": "cmake",
      "name": "tdlib-prepare",
      "src_dir": "libs/vendor/tdlib",
      "make_args": "prepare_cross_compiling",
      "dependencies_target": [
        "gperf"
      ]
    },
    {
      "template": "cmake",
      "name": "tdlib",
      "src_dir": "libs/vendor/tdlib",
      "make_jobs": "1",
      "make_args": "tdjson",
      "dependencies_target": [
        "zlib1g-dev",
        "libssl-dev",
        "gperf"
      ],
      "build_args": "-DTHREADS_PTHREAD_ARG=0 -DCMAKE_CROSSCOMPILING=ON -DCMAKE_BUILD_TYPE=Release"
    }
  ]
}
