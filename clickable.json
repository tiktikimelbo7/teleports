{
  "template": "cmake",
  "kill": "teleports",
  "dependencies": [
    "gperf",
    "libssl-dev",
    "ccache"
  ],
  "specificDependencies": true,
  "dir": "build/teleports",
  "make_jobs": "4",
  "libraries": [
    {
      "template": "custom",
      "name": "tdlib-prepare",
      "dependencies": [
        "gperf"
      ],
      "build": "cmake ../../../libs/vendor/tdlib && cmake --build . --target prepare_cross_compiling"
    },
    {
      "template": "cmake",
      "name": "tdlib",
      "src_dir": "libs/vendor/tdlib",
      "make_jobs": "1 tdjson",
      "dependencies": [
        "zlib1g-dev",
        "libssl-dev",
        "gperf"
      ],
      "build_args": "-DTHREADS_PTHREAD_ARG=0 -DCMAKE_CROSSCOMPILING=ON -DCMAKE_BUILD_TYPE=Release"
    }
  ]
}
